next(o)
{
	o.value = o.value + o.inc;
	return o.value;
}

iterator(inc)
{
	o = obj();
	o.value = 0;
	o.inc = inc;
	o.next = next;
	return o;
}

main5()
{
	it = iterator(2);
	print(it.next(it), "\n");
	print(it.next(it), "\n");
	print(it.next(it), "\n");
	print(it.next(it), "\n");
}



