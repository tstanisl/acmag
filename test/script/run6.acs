main2() {
	a = obj();
	n = 0;
	while (n < 10) {
		a[2 * n] = n * n;
		n = n + 1;
	}
	n = 0;
	while (n < 25) {
		print("a[", n, "] = ", a[n], "\n");
		n = n + 1;
	}
}

